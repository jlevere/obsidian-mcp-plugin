{
  "name": "obsidian-mcp-plugin",
  "version": "0.0.4",
  "description": "Allow an AI to interact with your notes in Obsidian via MCP",
  "main": "dist/main.js",
  "types": "main.d.ts",
  "engines": {
    "node": "^22.14.0"
  },
  "packageManager": "pnpm@10.8.0",
  "scripts": {
    "dev": "npx tsx esbuild.config.ts watch",
    "build": "pnpm tsc -noEmit -skipLibCheck && npx tsx esbuild.config.ts production",
    "build:dev": "pnpm tsc -noEmit -skipLibCheck && npx tsx esbuild.config.ts",
    "test": "pnpm jest",
    "inspect": "npx @modelcontextprotocol/inspector pnpm node main.js",
    "package": "pnpm build && zip plugin.zip dist/main.js manifest.json"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@esbuild-plugins/tsconfig-paths": "^0.1.2",
    "@modelcontextprotocol/inspector": "^0.11.0",
    "@modelcontextprotocol/sdk": "^1.10.2",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.14.1",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "builtin-modules": "^5.0.0",
    "esbuild": "0.25.2",
    "eslint": "^9.24.0",
    "jest": "^29.7.0",
    "obsidian": "^1.8.7",
    "tailwindcss": "^4.1.4",
    "ts-jest": "^29.3.2",
    "tslib": "2.8.1",
    "tsx": "^4.19.4",
    "typescript": "5.8.3"
  },
  "dependencies": {
    "@dmitryrechkin/json-schema-to-zod": "^1.0.1",
    "@sanity/diff-match-patch": "^3.2.0",
    "ajv": "^8.17.1",
    "express": "^5.1.0",
    "js-yaml": "^4.1.0",
    "query-string": "^7.1.3",
    "roarr": "^7.21.1",
    "zod": "^3.24.2"
  }
}