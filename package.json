{
  "name": "obsidian-mcp-plugin",
  "version": "0.0.5",
  "description": "Allow an AI to interact with your vault's notes through a self-contained MCP server",
  "main": "dist/main.js",
  "types": "main.d.ts",
  "engines": {
    "node": "^22.14.0"
  },
  "packageManager": "pnpm@10.8.0",
  "scripts": {
    "dev": "npx tsx esbuild.config.ts watch",
    "build": "pnpm tsc -noEmit -skipLibCheck && npx tsx esbuild.config.ts production",
    "build:dev": "pnpm tsc -noEmit -skipLibCheck && npx tsx esbuild.config.ts",
    "test": "pnpm jest",
    "lint": "pnpm eslint",
    "inspect": "npx @modelcontextprotocol/inspector pnpm node main.js",
    "package": "pnpm build && zip plugin.zip dist/main.js manifest.json"
  },
  "keywords": [
    "obsidian",
    "mcp",
    "ai",
    "plugin"
  ],
  "author": "jlevere",
  "license": "MIT",
  "devDependencies": {
    "@esbuild-plugins/tsconfig-paths": "^0.1.2",
    "@eslint/js": "^9.26.0",
    "@modelcontextprotocol/inspector": "^0.11.0",
    "@modelcontextprotocol/sdk": "^1.10.2",
    "@types/diff": "^7.0.2",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.14.1",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "builtin-modules": "^5.0.0",
    "esbuild": "0.25.2",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.3",
    "jest": "^29.7.0",
    "jiti": "^2.4.2",
    "obsidian": "^1.8.7",
    "tailwindcss": "^4.1.4",
    "ts-jest": "^29.3.2",
    "tslib": "2.8.1",
    "tsx": "^4.19.4",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.32.0"
  },
  "dependencies": {
    "@dmitryrechkin/json-schema-to-zod": "^1.0.1",
    "ajv": "^8.17.1",
    "diff": "^7.0.0",
    "express": "^5.1.0",
    "js-yaml": "^4.1.0",
    "zod": "^3.24.2"
  }
}